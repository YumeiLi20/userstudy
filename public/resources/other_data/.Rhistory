library(tidyverse)
library(stringr)
library(magrittr)
depts <- read_csv("raw_depts.csv", col_names = F)
depts <-
depts %>%
mutate(depts = str_extract(X1, pattern = ">[A-Z]{4} - [A-z 0-9 :punct:]*<"),
four_code = str_extract(X1, pattern = ">[A-Z]{4}"),
four_code = str_sub(four_code,2)) %>%
select(four_code) %T>%
View()
depts %>%
write_csv("dept_list.csv")
